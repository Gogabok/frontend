{
  "name": "social-frontend",
  "version": "0.1.0-dev",
  "private": true,
  "scripts": {
    "build:all": "yarn build:client && yarn build:server",
    "build:client": "webpack --config webpack/client.config.js && make yarn.write.hash",
    "build:server": "webpack --config webpack/server.config.js",
    "start:dev": "cross-env IS_DEV_SERVER=1 webpack-dev-server --config webpack/client.config.js --host 0.0.0.0",
    "build:android": "tns build android --bundle",
    "build:ios": "tns build ios --bundle",
    "debug:android": "tns debug android --bundle",
    "debug:ios": "tns debug ios --bundle",
    "docs": "typedoc --excludeExternals src",
    "gen:graphql": "apollo codegen:generate ./src/api/graphql/types.ts --queries=./src/api/graphql/**/*.ts --localSchemaFile=graphql.schema.json --target=typescript --outputFlat --passthroughCustomScalars --customScalarsPrefix=GraphQLScalars.",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:js": "standard --max-warnings 0",
    "lint:ts": "eslint {src,test}/**/*.ts --cache --max-warnings 0",
    "run:android": "tns run --hmr android --bundle",
    "run:ios": "tns run --hmr ios --bundle",
    "test:e2e": "yarn test:e2e:chrome && yarn test:e2e:ssr",
    "test:e2e:chrome": "wait-on -t 120000 http://localhost && cypress run --browser chrome",
    "test:e2e:interactive": "cypress open",
    "test:e2e:ssr": "wait-on -t 120000 http://localhost && cypress run --config userAgent=Googlebot,integrationFolder=test/e2e/specs_ssr",
    "test:unit": "jest --config=test/unit/jest.config.js",
    "watch:test:unit": "nodemon --exec 'yarn test:unit'"
  },
  "browserslist": [
    "ie 11",
    "edge >= 15",
    "firefox >= 55",
    "chrome >= 60",
    "safari >= 10.1",
    "opera >= 48",
    "ios >= 10.2",
    "operamini all",
    "android >= 4.4",
    "chromeandroid >= 61",
    "ucandroid >= 11.4",
    "samsung >= 4"
  ],
